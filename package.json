  "scripts": {
    "asm-dom-js": "cd asm-dom-js && npx webpack serve",
    "asm-dom-cpp-build": "cmake --preset emsdk && cmake --build build --config Release --target asm-dom-cpp",
    "asm-dom-cpp": "npm run asm-dom-cpp-build && emrun --port 9000 --no-browser build/asm-dom-cpp/index.html",
    "virtual-dom-js": "cd virtual-dom-js && npx webpack serve",
    "wasm-dom-js-build": "cmake --preset emsdk && cmake --build build --config Release --target wasm-dom-js",
    "wasm-dom-js": "npm run wasm-dom-js-build && cd wasm-dom-js && npx webpack serve",
    "wasm-dom-cpp-build": "cmake --preset emsdk && cmake --build build --config Release --target wasm-dom-cpp",
    "wasm-dom-cpp": "npm run wasm-dom-cpp-build && emrun --port 9000 --no-browser build/wasm-dom-cpp/index.html"
  },
  "dependencies": {
    "asm-dom": "^0.7.0",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^13.0.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
